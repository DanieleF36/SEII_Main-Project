{
  "scripts": {
    "start": "npx nodemon index.js",
    "test": "jest",
    "test:coverage": "jest --runInBand --coverage --colors",
    "test:unit": "npx jest --runInBand --coverage --coverageDirectory=coverage/coverage_unit --colors auth.unit.test.js controller.unit.test.js users.unit.test.js utils.unit.test.js",
    "test:integration": "set test=true && jest",
    "test:integrationCoverage": "npx jest --runInBand --coverage --coverageDirectory=coverage/coverage_integration --colors auth.integration.test.js controller.integration.test.js users.integration.test.js utils.integration.test.js",
    "test:official": "npx jest --runInBand --coverage --colors --coverageDirectory=coverage/coverage_official --testPathPattern=test_official/.*\\.test\\.js$"
  },
  "dependencies": {
    "ajv-formats": "^2.1.1",
    "connect": "^3.2.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-json-validator-middleware": "^3.0.1",
    "express-session": "^1.17.3",
    "express-static": "^1.2.6",
    "express-validator": "^7.0.1",
    "formidable": "^3.5.1",
    "jest": "^29.7.0",
    "js-yaml": "^3.3.0",
    "jsonschema": "^1.4.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "nodemon": "^3.0.1",
    "oas3-tools": "^2.2.3",
    "passport": "^0.6.0",
    "passport-saml": "^3.2.4",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
